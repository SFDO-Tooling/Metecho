{
  "name": "metashare",
  "description": "Web-based tool for collaborating on Salesforce projects",
  "version": "0.1.0",
  "private": true,
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/SFDO-Tooling/MetaShare.git"
  },
  "engines": {
    "node": "12.14.1"
  },
  "scripts": {
    "webpack:serve": "webpack-dev-server --config webpack.dev.js",
    "django:serve": "python manage.py runserver 0.0.0.0:${PORT:-8000}",
    "django:serve:prod": "daphne --bind 0.0.0.0 --port ${PORT:-8000} metashare.asgi:application",
    "redis:clear": "redis-cli -h redis FLUSHALL",
    "worker:serve": "python manage.py rqworker default",
    "rq:serve": "npm-run-all redis:clear -p worker:serve",
    "serve": "run-p django:serve webpack:serve rq:serve",
    "prettier:js": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "prettier:other": "prettier --write '**/*.{json,md,yml}'",
    "eslint": "yarn eslint:nofix --fix",
    "eslint:nofix": "eslint '*.js' src/js test/js --ext .js,.ts,.jsx,.tsx",
    "tsc": "tsc",
    "flake8": "flake8 manage.py metashare/ config/",
    "isort": "isort -rc manage.py metashare/ config/",
    "black": "black manage.py metashare/ config/",
    "lint": "run-p lint:py lint:js lint:sass prettier:other",
    "lint:nofix": "run-p flake8 eslint:nofix lint:sass:nofix tsc",
    "lint:js": "run-s prettier:js eslint tsc",
    "lint:py": "run-s isort black flake8",
    "lint:sass:nofix": "stylelint '**/*.scss'",
    "lint:sass": "yarn lint:sass:nofix --fix",
    "test:js": "jest",
    "test:js:watch": "jest --watchAll",
    "test:js:coverage": "JEST_JUNIT_OUTPUT_DIR='./coverage' jest --ci --runInBand --reporters=default --reporters=jest-junit",
    "test:js:report-coverage": "cat ./coverage/lcov.info | coveralls",
    "test:js:check-coverage": "nyc check-coverage --temp-dir coverage",
    "test:py": "pytest",
    "test:py:report-coverage": "$VIRTUAL_ENV/bin/coveralls",
    "test:py:check-coverage": "coverage3 report --fail-under=100",
    "test": "run-s test:js test:py",
    "build": "webpack --config webpack.dev.js",
    "prod": "webpack --config webpack.prod.js"
  },
  "dependencies": {
    "@salesforce-ux/design-system": "2.11.2",
    "@salesforce/design-system-react": "0.10.17",
    "@sentry/browser": "^5.11.1",
    "classnames": "^2.2.6",
    "date-fns": "^2.9.0",
    "i18next": "^19.0.3",
    "i18next-browser-languagedetector": "^4.0.1",
    "i18next-xhr-backend": "^3.2.2",
    "js-cookie": "^2.2.1",
    "lodash.clonedeep": "^4.5.0",
    "react": "^16.12.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.12.0",
    "react-fns": "^1.4.0",
    "react-i18next": "^11.3.1",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "sockette": "^2.0.6",
    "uuid": "^3.4.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@testing-library/jest-dom": "^5.0.2",
    "@testing-library/react": "^9.4.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/classnames": "^2.2.9",
    "@types/core-js": "^2.5.2",
    "@types/js-cookie": "^2.2.4",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/react": "^16.9.19",
    "@types/react-document-title": "^2.0.3",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-logger": "^3.0.7",
    "@types/uuid": "^3.4.6",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "autoprefixer": "^9.7.4",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.6.4",
    "coveralls": "^3.0.9",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-simple-import-sort": "^5.0.1",
    "fetch-mock": "^8.3.1",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "i18next-scanner-webpack": "^0.4.4",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.0.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.13.0",
    "svg-inline-loader": "^0.8.0",
    "typescript": "^3.7.5",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  },
  "resolutions": {
    "@types/vfile-message": "1.0.1"
  }
}
