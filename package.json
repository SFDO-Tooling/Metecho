{
  "name": "metashare",
  "description": "Web-based tool for collaborating on Salesforce projects",
  "version": "0.1.0",
  "private": true,
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/SFDO-Tooling/MetaShare.git"
  },
  "engines": {
    "node": "10.16.0"
  },
  "scripts": {
    "webpack:serve": "webpack-dev-server --config webpack.dev.js",
    "django:serve": "python manage.py runserver 0.0.0.0:${PORT:-8000}",
    "django:serve:prod": "daphne --bind 0.0.0.0 --port ${PORT:-8000} metashare.asgi:application",
    "redis:clear": "redis-cli -h redis FLUSHALL",
    "worker:serve": "python manage.py rqworker default",
    "scheduler:serve": "python manage.py rqscheduler --queue default",
    "rq:serve": "npm-run-all redis:clear -p worker:serve scheduler:serve",
    "serve": "run-p django:serve webpack:serve rq:serve",
    "prettier": "prettier --write '*.js' '.*.js' 'src/js/*.ts' 'src/js/*.tsx' 'src/js/**/*.ts' 'src/js/**/*.tsx' 'test/js/*.js' 'test/js/*.jsx' 'test/js/**/*.js' 'test/js/**/*.jsx' 'src/sass/**/*.scss'",
    "eslint": "eslint '*.js' 'src/js/*.ts' 'src/js/*.tsx' 'src/js/**/*.ts' 'src/js/**/*.tsx' 'test/js/*.js' 'test/js/**/*.js' 'test/js/**/*.jsx'",
    "tsc": "tsc",
    "stylelint": "stylelint 'src/sass/**/*.scss'",
    "flake8": "flake8 manage.py metashare/ config/",
    "isort": "isort -rc manage.py metashare/ config/",
    "black": "black manage.py metashare/ config/",
    "lintpy": "run-s isort black flake8",
    "lintjs": "run-s prettier eslint tsc",
    "lint": "npm-run-all -p lintpy lintjs -s stylelint",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "JEST_JUNIT_OUTPUT='./coverage/junit.xml' jest --ci --runInBand --reporters=default --reporters=jest-junit",
    "test:report-coverage": "cat ./coverage/lcov.info | coveralls",
    "test:check-coverage": "nyc check-coverage --temp-directory coverage",
    "pytest": "pytest",
    "pytest:report-coverage": "$VIRTUAL_ENV/bin/coveralls",
    "pytest:check-coverage": "coverage3 report --fail-under=100",
    "test:all": "run-s test pytest",
    "build": "webpack --config webpack.dev.js",
    "prod": "webpack --config webpack.prod.js",
    "heroku-postbuild": "yarn prod"
  },
  "dependencies": {
    "@salesforce-ux/design-system": "^2.9.4",
    "@salesforce/design-system-react": "^0.10.9",
    "@sentry/browser": "^5.6.1",
    "classnames": "^2.2.6",
    "i18next": "^17.0.10",
    "i18next-browser-languagedetector": "^3.0.3",
    "i18next-xhr-backend": "^3.1.1",
    "js-cookie": "^2.2.1",
    "react": "^16.9.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.9.0",
    "react-fns": "^1.4.0",
    "react-i18next": "^10.11.5",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "sockette": "^2.0.6",
    "uuid": "^3.3.2",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^8.0.9",
    "@types/classnames": "^2.2.9",
    "@types/core-js": "^2.5.2",
    "@types/js-cookie": "^2.2.2",
    "@types/react": "^16.9.1",
    "@types/react-document-title": "^2.0.3",
    "@types/react-dom": "^16.8.5",
    "@types/react-redux": "^7.1.1",
    "@types/react-router-dom": "^4.3.4",
    "@types/redux-logger": "^3.0.7",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "autoprefixer": "^9.6.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.2.0",
    "coveralls": "^3.0.6",
    "css-loader": "^3.2.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-simple-import-sort": "^4.0.0",
    "fetch-mock": "^7.3.9",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "i18next-scanner-webpack": "^0.4.2",
    "jest": "^24.8.0",
    "jest-junit": "^7.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.2.0",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-scss": "^3.9.3",
    "typescript": "^3.5.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.1"
  }
}
